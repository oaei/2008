  <owl:ObjectProperty rdf:ID="pagesOrChapter">
    <rdfs:domain>
      <owl:Class>
	<owl:unionOf rdf:parseType="Collection">
	  <owl:Class rdf:about="#Part" />
	</owl:unionOf>
      </owl:Class>
