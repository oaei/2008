        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="&xsd;string" />
    <rdfs:subPropertyOf rdf:resource="#numberOrVolume" />
    <rdfs:label xml:lang="en">Issue</rdfs:label>
